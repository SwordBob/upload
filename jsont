<script>
var gettype= Object.prototype.toString;
alert(eval(ret=[{id:1,node:[{id:2,node:[{id:3}]}]},{id:4,node:[{id:5,node:[{id:6,edit:1}]}]},{id:41,node:[{id:51,node:[{id:61,edit:1}]}]}]))
